- for(var key in items){
    .panel.panel-info
        .panel-heading
            =lan_mapping[key]
        ul.list-group
            -items[key].forEach(function(item){
                - if(item.checked){
                    li.active.list-group-item
                        a(href="#" onclick="$('#frame').attr('src','#{item.target_url}&token=#{user.token_id}&from=cassite.net&lan=#{lan.is}');")
                            =lan_mapping[item.title]
                -}else{
                    li.list-group-item
                        a(href="#" onclick="$('#frame').attr('src','#{item.target_url}&token=#{user.token_id}&from=cassite.net&lan=#{lan.is}');")
                            =lan_mapping[item.title]
                -}
            -})
- }
button.btn.btn-sm.btn-default(onclick="window.open($('#frame').attr('src'))") #{lan.button.show_inner_in_new_window}